var w = 0;
var h = 0;
var p ='';
function deadCenter(p,w,h) {
        if (window.screen) {
var chasm = screen.availWidth;
var mount = screen.availHeight;
  putItThere = window.open(p,'posB','width=' + w + ',height=' + h + ',left=' + ((chasm - w - 10) * .5) + ',top=' + ((mount - h - 30) * .5));
        }
}
