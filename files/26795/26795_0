//關閉彈跳選單
function menu_close(id,e,obj){
	var element=document.getElementById(id);
	var height=0;
	var top=0;
	var left=0;
	var right=0;
	//阻止左邊邊界事件冒泡
	var mx=getMouseX(e);
	var my=getMouseY(e);
	if( !-[1,] ){ //IE  
		height=element.offsetTop+element.offsetHeight-document.documentElement.scrollTop-6;
		top=element.offsetTop-document.documentElement.scrollTop+6;
		left=element.offsetLeft+6;
		right=element.offsetLeft+element.offsetWidth;
	}
	else{
		height=element.offsetTop+element.offsetHeight-document.documentElement.scrollTop-2;
		top=element.offsetTop-document.documentElement.scrollTop;
		left=element.offsetLeft+6;
		right=element.offsetLeft+element.offsetWidth;
	}
	
	if(mx>right || my>height || my<top){
		element.style.display='none';
	}	
	
	//阻止事件冒泡
	if( !-[1,] ) //IE  
	{  
		var readonly = "readOnly";  
		var objs = window.event.srcElement;
		if(obj!=objs){
			window.event.cancelBubble=true;
			return;
		}
	}  
	else 
	{   
		var readonly = "readonly"; 
		var objs = e.target;
		if(obj!=objs){
			stopPropagation();
			return;
		}
	}
	
	if(mx <left)
		return;
	//阻止右邊邊界事件冒泡
	if(mx >left && mx<right){
		if(my<=height && my>=top)
			return;	
	}
	element.style.display='none';
}
//root關閉彈跳選單
function menu_root_close(id,e,obj){
	var element=document.getElementById(id);
	var height=0;
	var top=0;
	var left=0;
	var right=0;
	var mx=getMouseX(e);
	var my=getMouseY(e);
	if( !-[1,] ){ //IE  
		height=element.offsetTop+element.offsetHeight-document.documentElement.scrollTop-6;
		top=element.offsetTop-document.documentElement.scrollTop+6;
		left=element.offsetLeft;
		right=element.offsetLeft+element.offsetWidth;
	}
	else{
		height=element.offsetTop+element.offsetHeight-document.documentElement.scrollTop-2;
		top=element.offsetTop-document.documentElement.scrollTop;
		left=element.offsetLeft;
		right=element.offsetLeft+element.offsetWidth;
	}	
	//阻止右邊邊界事件冒泡
	if(mx >=left){
			return;	
	}
	element.style.display='none';
}
//關閉所有彈跳選單
