<script type="text/javascript">
$(function(){
var $block = $('#TPH201605230357_bannerBlock'),
	$slides = $('ul.list', $block),
	TPH201605230357_width = $slides.find('li').width(),
	TPH201605230357_animateSpeed = 1000,
	TPH201605230357_stop = false,
	bannerTurn = '',
	TPH201605230357_rightSize = ($slides.find('li').size()*TPH201605230357_width-$block.width())*-1;
	// 移動主函式
	function move(){
		if(bannerTurn=='right'){
			$slides.css({left: TPH201605230357_rightSize});
			$slides.stop().animate({
				left: '+='+TPH201605230357_width
			}, TPH201605230357_animateSpeed, function(){
				$slides.stop().css({left: TPH201605230357_rightSize});
				$slides.find('li:last').prependTo($slides);
				if(!TPH201605230357_stop){
					window.setTimeout(function(){move()},5000);
				}
			});
		}else{
			$slides.css({left: ''});
			$slides.stop().animate({
				left: '-='+TPH201605230357_width
			}, TPH201605230357_animateSpeed, function(){
				$slides.stop().css({left: ""});
				$slides.find('li').eq(0).appendTo($slides);
				if(!TPH201605230357_stop){
					window.setTimeout(function(){move()},5000);
				}
			});
		}
	}
	// 移動列滑鼠事件
	$slides.hover(function(){
		TPH201605230357_stop = true;
	}, function(){
		TPH201605230357_stop = false;
		move();
	});
	// 向左鍵事件
	$('#TPH201605230357_abgneBlock .prevten').click(function(){$slides.stop();bannerTurn='left';move();});
	// 向右鍵事件
	$('#TPH201605230357_abgneBlock .next').click(function(){$slides.stop();bannerTurn='right';move();});
move();
});
</script>
 <P align=center><IMG alt=影音專區 src="http://www.moj.gov.tw/site/moj/public/MMO/tph/影音專區BANNER.png" mmoid="417204"><A title=影音專區 href="http://www.tph.moj.gov.tw/lp.asp?CtNode=42829&CtUnit=15029&BaseDSD=7&mp=003" target=_gipNW>更多影片</A><BR><IFRAME title=影片宣導 height=250 src="//www.youtube.com/embed/6CmPevfuF_Y" frameBorder=0 width=300 allowfullscreen></IFRAME>&nbsp;&nbsp; <IFRAME title=影片宣導 height=250 src="//www.youtube.com/embed/qjNIQXzfoyE" frameBorder=0 width=300 allowfullscreen></IFRAME>&nbsp;&nbsp; <IFRAME title=影片宣導 height=250 src="//www.youtube.com/embed/uHps_7RdPOM" frameBorder=0 width=300 allowfullscreen></IFRAME></P></div>
