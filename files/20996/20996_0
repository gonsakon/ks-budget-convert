				function shareData(site)
				{
					switch(site){
						case "fb":
							document.getElementById(site).href = "http://www.facebook.com/sharer.php?u="+encodeURIComponent(window.location.toString());
							break;
						case "pk":
							document.getElementById(site).href = "http://www.plurk.com/?qualifier=shares&status="+encodeURIComponent(window.location.toString()+" ()");
							break;
						case "twi":
							document.getElementById(site).href = "http://twitter.com/home/?status="+encodeURIComponent(" "+window.location.toString());
							break;
						case "gp":
