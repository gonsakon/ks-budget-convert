                    </style>
                    <div style="color: #ffffff; float: right;">
                        當Script無法執行搜尋功能時，
                        請直接進入<a accesskey="S" href="/sites/ch/search" style="color: #ffd800">搜尋頁面</a>使用搜尋功能。
                    </div>
                </noscript>
                <script>
                    $("#btnSearch").click(function () {
                        var input = $('#txbSearchKeyword');
                        var keyword = input.val();
                        var visible = input.is(":visible");

                        if (!visible) $("#txbSearchKeyword").toggle();
                        else if (keyword == "") {
                            alert("請輸入關鍵字");
                            return;
                        }
                        else {
                            location.href = '/Search.aspx?q=' + keyword;
                        }
                    });

                $("#txbSearchKeyword").keydown(function () {

                    var btnSearch = $("#btnSearch")

                    if (event.keyCode == 13) {
                        btnSearch.focus();
                    }
                });
                </script>
