            <script>
                var li = '#mainCollapse > ul > li';
                var isTabEnter = false;

                $(li).click(function () {
                    if (!$(this).hasClass('active'))
                        $(li).removeClass('active');

                    $(this).toggleClass('active');
                });

                $('a[data-type="mainNode"]').on("keydown", function (event) {
                    if (event.which == 13) {
                        isTabEnter = true;
                    }
                });

                $('a[data-type="mainNode"]').click(function (e) {
                    if (isTabEnter == false)
                        e.preventDefault();
                });
            </script>
