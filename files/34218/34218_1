	<script type="text/javascript">

      function slideLeftOut()
	  {
		var left = $('floatHeader').offsetLeft		
		if (left < 0 )
		{
		    new Effect.Move('floatHeader',{ x: 1502, y: 0});
		}
	  }
	  
	  function slideRightOut()
	  {
		var left = $('floatHeader').offsetLeft		
		if (left > 0 )
		{
		    new Effect.Move('floatHeader',{ x: -1502, y: 0});
		}
	  }
	  
	  function resetPersonalCheckBoxList()
	  {
	    var divCheckBox = $('checkboxList');
	    var allInput = divCheckBox.getElementsByTagName("input");       
	    for (var i = 0; i< allInput.length; i++)
	    {
	        allInput[i].checked = true;
	    }
	    setCookie( "workItem", null, -1, "/" );	 
	    setCookie( "menuset", null, -1, "/" );	    
	    
	    top.location.href = "../Default.aspx";
	  }
	  
	  function SaveResult()
	  {
	    var divCheckBox = $('checkboxList');
	    var allInput = divCheckBox.getElementsByTagName("input"); 
	    var checkValue = "";      
	    for (var i = 0; i< allInput.length; i++)
	    {
	        if (allInput[i].checked)
	        {
	            if (checkValue.length > 0)
	            {
	                checkValue += ",";
	            }
	            
	            if (allInput[i].value != "undefined")
	            {
	                checkValue += allInput[i].value;
	            }
	            
	        }       
	        
	    }
	    
	    if (checkValue != null)
	    {
	        setCookie( "workItem", null, -1, "/" );	
	        setCookie( "menuset", "", -1, "/" );
	        setCookie( "menuset", checkValue, 7, "/" );
	    }
	    
	    //slideRightOut();
	    top.location.href = "../Default.aspx";
	  
	  }
	  
	  function SaveWorkItemCookie(workItemId, path)
	  {
	    setCookie( "workItem", null, -1, "/" );	
	    setCookie( "workItem",workItemId , "", "/" );	
	    window.location.href = path;
	  }
	    
	  
	  
	   // ]]>
</script>
	<script type="text/javascript">
	dw_fontSizerDX.init();
	</script>	
