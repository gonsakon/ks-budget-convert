    function T(box_name) {
        if (document.getElementById(box_name)) {

            if (document.getElementById(box_name).style.display == "none") {
                ii();
                document.getElementById(box_name).style.display = "";
            }
            else {
                document.getElementById(box_name).style.display = "none";
            }
        }
    }

