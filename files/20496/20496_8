<script  >
    function doSearch() {
        var KeyWord = $('#tagsKeyWords').val();
            if(KeyWord.trim()=='')
            return false;
            else
        window.location.href = "Advanced_Search.aspx?q=" + KeyWord;
    }
$(function () {
    $("#tagsKeyWords").on("keydown", function (event) {
        if (event.which == 13) {
            var KeyWord = $('#tagsKeyWords').val();
            if(KeyWord.trim()=='')
            return false;
            else
            window.location.href = "Advanced_Search.aspx?q=" + KeyWord;
            return false;
        }
    });
});
</script>
