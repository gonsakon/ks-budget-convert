<script language="javascript">
	function ExecFind()
	{
		var NowForm = document.forms[0];
		
		if(NowForm.SearchWord.value == '請輸入關鍵字') NowForm.SearchWord.value = '';
		
		if(NowForm.SearchWord.value == '')
		{
			alert('請輸入關鍵字!');
		}
		else
		{
			location.href = 'search.aspx?ID=$0120&IDK=2&AP=$0120_SW-' + encodeURI(NowForm.SearchWord.value);
		}
	}

	function CheckEnter()
	{
		if (event.keyCode == 13) 
		{        
			event.cancelBubble = true;
			event.returnValue = false;

			ExecFind();
         	}	
	}
</script>
