<script type="text/javascript">
$(document).ready(function(){
  
  $("#small").click(function(event){
    event.preventDefault();
    $("h1").animate({"font-size":"15px"});
    $("h2").animate({"font-size":"13px"});
    $("p").animate({"font-size":"11px", "line-height":"15px"});
    $(".Center").animate({"font-size":"11px", "line-height":"16px"});
  });
  
  $("#medium").click(function(event){
    event.preventDefault();
    $("h1").animate({"font-size":"17px"});
    $("h2").animate({"font-size":"15px"});
    $("p").animate({"font-size":"13px", "line-height":"17px"});
	$(".Center").animate({"font-size":"13px", "line-height":"18px"});
    
  });
  
  $("#large").click(function(event){
    event.preventDefault();
    $("h1").animate({"font-size":"19px"});
    $("h2").animate({"font-size":"17px"});
    $("p").animate({"font-size":"15px", "line-height":"20px"});
	$(".Center").animate({"font-size":"15px", "line-height":"22px"});
    
  });
  
  $( "a" ).click(function() {
   $("a").removeClass("selected");
  $(this).addClass("selected");
  
 });

});
</script>


