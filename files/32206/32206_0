<script language="javascript">
//<!--
	  var $0001_listMenu  = new Array();
	  var $0001_listTable = new Array();
	  var $0001_TotMenu   = 0;
	  var $0001_IsHorz    = false;
	  var $0001_IsLeftTop = true;

      function Point(_x, _y)
      {
      　　this.x = _x;
      　　this.y = _y;
      }

      function $0001_realPosition(_obj) 
	  {
     　　 var currPos = new Point(_obj.offsetLeft,_obj.offsetTop);
      　　var workPos = new Point(0,0);
      　　if (_obj.offsetParent.tagName != "BODY")
      　　{
      　　　　workPos = $0001_realPosition(_obj.offsetParent);
      　　　　currPos.x += workPos.x;
      　　　　currPos.y += workPos.y;
      　　}
		  else
		  {	  
	  	  	  currPos.x += parseInt(document.body.leftMargin);
		  	  currPos.y += parseInt(document.body.topMargin);
		  }	

      　　return currPos;
      }
	  
	  function $0001_realSize(_obj)
	  {
	  	  return (new Point(_obj.offsetWidth,_obj.offsetHeight));	
	  }
	  
	  function $0001_RegisterMenu(objMenu, objList)
	  {
	  	  $0001_TotMenu = $0001_TotMenu + 1;
		  $0001_listMenu[$0001_TotMenu - 1] = objMenu;
		  $0001_listTable[$0001_TotMenu - 1] = objList;
	  }
	  
	  function $0001_ShowMenu(objItem, objMenu)
	  {
	      var objTable = null;
	  	  var NowPos   = $0001_realPosition(objItem);
		  var NowSize  = $0001_realSize(objItem);
		  
		  $0001_HideAllMenu();
		  
		  for(i=0; i<$0001_TotMenu; i++)
		  {
		       if(objMenu == $0001_listMenu[i])
			   {
			        objTable = $0001_listTable[i];
					break;
			   }    	
		  }	

		  if(!objTable)
		  {
		       // None
		  }
		  else
		  { 
		       objMenu.style.width  = objTable.offsetWidth;
		       objMenu.style.height = objTable.offsetHeight;
		  
		       objMenu.style.visibility = 'visible';
		  } 	
	  }
	  
	  function $0001_HideMenu(objMenu)
	  {
		   if(!objMenu)
		   {
				// None	
		   }
		   else if(objMenu.style.visibility == 'hidden')
		   {
				// None	
		   }
		   else
		   {
				objMenu.style.visibility = 'hidden';
		   }	
	  }
	  
	  function $0001_HideAllMenu()
	  {
	      var objMenu;  	
	  
	  	  for(i=0; i<$0001_TotMenu; i++)
		  {
		  	   objMenu = $0001_listMenu[i];
			   $0001_HideMenu(objMenu);	
		  } 
	  }
	  
	  function $0001_onMouseOutMenu()
	  {
	  	  var x = event.x;
		  var y = event.y;
		  var IsFound = false;
		  
		  for(i=0; i<$0001_TotMenu; i++)
		  {
		  	   objMenu = $0001_listMenu[i];
			   if(!objMenu)
			   {
					// None
			   }
			   else if(objMenu.style.visibility == 'hidden')
			   {
					// None
			   }
			   else if((x >= parseInt(objMenu.style.left)) && (y >= parseInt(objMenu.style.top)) &&
			            (x <= (parseInt(objMenu.style.left) + parseInt(objMenu.style.width))) &&
				        (y <= (parseInt(objMenu.style.top ) + parseInt(objMenu.style.height))))
			   {
			   		IsFound = true;
					break;
			   }			
		  }
		  
		  if(!IsFound)
		  {
		       $0001_HideAllMenu();
		  }
	  }
	
	  function $0001_OpenManage()
	  {
		var wx = 650;
		var wy = 500;
	
		var sx = screen.width;
		var sy = screen.height;
	
		var NewX = (sx - wx) / 2;
		var NewY = (sy - wy) / 2;
	
		var NowURL = "MenuIList.aspx?CI=$0001";
		window.open(NowURL, 
					"MenuIList",
					"left="   + NewX + ","
				  + "top="    + NewY + "," 
				  + "width="  + wx + ","
				  + "height=" + wy + ","
				  + "toolbar=no,location=no,status=no,scrollbars=yes");
	  }
	  
	  function $0001_ChangeImage(objItem, NewItem)
	  {
	  	if(!objItem)
		{
			// None
		}
		else
		{
			objItem.background = NewItem;
		}
	  }
	  
	  function $0001_ChangeImage2(objItem, NewItem)
	  {
	  	if(!objItem)
		{
			// None
		}
		else
		{
			objItem.src = NewItem;
		}
	  }
