<script type="text/javascript">
$(function(){
var $_block = $('#_bannerBlock'),
	$_slides = $('ul.list', $_block),
	$_width = $_slides.find('li').width(),
	$_animateSpeed = 600,
	$_animateTimeout = 15000,
	$_stop = false,
	bannerTurn = '',
	$_rightSize = ($_slides.find('li').size()*$_width-$_block.width())*-1;
	// 移動主函式
	function _move(){
		clearTimeout($_timer);
		if(bannerTurn=='right'){
			$_slides.css({left: $_rightSize});
			$_slides.stop().animate({
				left: '+='+$_block.width()
			}, $_animateSpeed, function(){
				$_slides.stop().css({left: $_rightSize});
				$_slides.find('li:last').prependTo($_slides);
				$_slides.find('li:last').prependTo($_slides);
				$_slides.find('li:last').prependTo($_slides);
				$_slides.find('li:last').prependTo($_slides);
				$_slides.find('li:last').prependTo($_slides);
				if(!$_stop){
					$_timer = setTimeout(_move, $_animateTimeout);
				}
			});
		}else{
			$_slides.css({left: ''});
			$_slides.stop().animate({
				left: '-='+$_block.width()
			}, $_animateSpeed, function(){
				$_slides.stop().css({left: ""});
				$_slides.find('li').eq(0).appendTo($_slides);
				$_slides.find('li').eq(0).appendTo($_slides);
				$_slides.find('li').eq(0).appendTo($_slides);
				$_slides.find('li').eq(0).appendTo($_slides);
				$_slides.find('li').eq(0).appendTo($_slides);
				if(!$_stop){
					$_timer = setTimeout(_move, $_animateTimeout);
				}
			});
		}
	}
	// 移動列滑鼠事件
	$_slides.hover(function(){
		$_stop = true;
	}, function(){
		$_stop = false;
		//_move();
	});
	// 向左鍵事件
	$('#_abgneBlock .prevten').click(function(){$_slides.stop();bannerTurn='left';_move();});
	// 向右鍵事件
	$('#_abgneBlock .next').click(function(){$_slides.stop();bannerTurn='right';_move();});

	$_timer = setTimeout(_move, $_animateTimeout);
	
});


</script>
 </div>
