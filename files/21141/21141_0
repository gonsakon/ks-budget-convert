  // document.onkeydown
  //document.onfocusin = focusin,
  document.onfocusin = function(e) {
    //alert(event.srcElement.title);
    if (event.srcElement.nodeName == "A") {
      if (event.srcElement.tabIndex == "10") {
        event.srcElement.style.backgroundColor='#FFF799';
      }
    }
  };
  document.onfocusout = function(e) {
    if (event.srcElement.nodeName == "A") {
      if (event.srcElement.tabIndex == "10") {
        event.srcElement.style.backgroundColor='';
      }
    }
  };
  
  function init() {
    // alert("init");
    var aTags = document.getElementsByTagName("a");
    for (var i in aTags) {
      //alert(aTags[i].className);
      if (aTags[i].className == "current") {
        //alert(aTags[i].title);
        aTags[i].focus();
      }
    }
    // add a tabindex="10" into ID jsn-mainbody
    aTags = document.getElementById("jsn-mainbody").getElementsByTagName('a');
    for (var i=0; i < aTags.length; i++) {
      if (aTags[i].nodeName == "A") {
      	if (aTags[i].name) aTags[i].tabIndex = "-1";  // no stop at <a name="top">
        else aTags[i].tabIndex = "10";
      }
    }
  }
</script>
<noscript>TAB標籤效果，如不執行不影響您資料瀏覽</noscript>