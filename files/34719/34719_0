<script type="text/javascript">
  //點閱率(Menu)
  function read(menuid,menutable) {
	  $.ajax({
		  url: "ap/updatenumber_menu.jsp",
		  type: "POST",
		  data: "menuid=" + menuid + "&menutable=" + menutable
	  });
  }	 
</script>


