  //點閱率(Menu)
  function readsub(menuid,menutable) {
	  $.ajax({
		  url: "ap/updatenumber_menu.jsp",
		  type: "POST",
		  data: "menuid=" + menuid + "&menutable=" + menutable
	  });
  }	 
