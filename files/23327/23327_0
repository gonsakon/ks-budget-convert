<script type="text/javascript">
$(function(){
var $block = $('#A1_bannerBlock'),
	$slides = $('ul.list', $block),
	A1_width = $slides.find('li').width(),
	A1_animateSpeed = 1000,
	A1_stop = false,
	bannerTurn = '',
	A1_rightSize = ($slides.find('li').size()*A1_width-$block.width())*-1;
	// 移動主函式
	function move(){
		if(bannerTurn=='right'){
			$slides.css({left: A1_rightSize});
			$slides.stop().animate({
				left: '+='+A1_width
			}, A1_animateSpeed, function(){
				$slides.stop().css({left: A1_rightSize});
				$slides.find('li:last').prependTo($slides);
				if(!A1_stop){
					window.setTimeout(function(){move()},5000);
				}
			});
		}else{
			$slides.css({left: ''});
			$slides.stop().animate({
				left: '-='+A1_width
			}, A1_animateSpeed, function(){
				$slides.stop().css({left: ""});
				$slides.find('li').eq(0).appendTo($slides);
				if(!A1_stop){
					window.setTimeout(function(){move()},5000);
				}
			});
		}
	}
	// 移動列滑鼠事件
	$slides.hover(function(){
		A1_stop = true;
	}, function(){
		A1_stop = false;
		move();
	});
	// 向左鍵事件
	$('#A1_abgneBlock .prevten').click(function(){$slides.stop();bannerTurn='left';move();});
	// 向右鍵事件
	$('#A1_abgneBlock .next').click(function(){$slides.stop();bannerTurn='right';move();});
move();
});
</script>
 <br>

