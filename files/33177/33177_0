			$(document).ready(function() {
				$("*").each(function() {
					if (typeof($(this).attr("style")) == "undefined") {
					 //empty
					} 
					 else {
						var str = $(this).attr("style");
						var matchs = str.match(/(^[\D])*width: (\d+)(px)/g);
			   var pm = /(^[\D])*([\w-]*width): (\d+)(px)/;
						var matches_array = str.match(matchs);
						if (matches_array != null) {
						 var answer = str.match(pm);
						  var result = answer[3] / 16;
				 $(this).css(answer[2], result + "em");
						}
					}
				});
			});
