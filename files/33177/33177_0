<script type="text/javascript">
	$(function() { 
		var ActH = $("#ActListContent").height();
		var ActR = $("#ActRightContent").height()+10;
		//alert(ActH);
		//alert(ActR);
		if(ActH < ActR)
			$("#ActContent").height(ActR);
		else
			$("#ActContent").height(ActH + 200);
			
		
		$(".ShowStyle1 table").each(function () {
			var tbor = $(this).attr("border");
			if(tbor != "0" && tbor != undefined)
			{
				$(this).addClass("TablebStyle");	
			}
		 });
		 
		 //判斷FOOTER是展開還是收合
		if(''==''||''=='SHOW')
		 {
			$("#toggleParentDiv").css("background-image","url(/images/btn01.png)");
			$("#footerDiv").show();
			$("#hideTable").hide();
		 }
		 else
		 {
			$("#toggleParentDiv").css("background-image","url(/images/btn02.png)");
			$("#footerDiv").hide();
			$("#hideTable").show();
		 }
		 
		 $("#toggleDiv").click(function(){
			$("#footerDiv").toggle();
			if($("#footerDiv").is(":visible"))
			{	
				SetSessionAjax('show');
				$("#hideTable").hide();
				$("#toggleParentDiv").css("background-image","url(/images/btn01.png)");
			}
			else
			{
				SetSessionAjax('hide');
				$("#hideTable").show();
				$("#toggleParentDiv").css("background-image","url(/images/btn02.png)");
			}
		 });
	});
	
	function SetSessionAjax(isVisible)
	{
		var xmlhttp;
		if (window.XMLHttpRequest)
		{// code for IE7+, Firefox, Chrome, Opera, Safari
		xmlhttp=new XMLHttpRequest();
		}
		else
		{// code for IE6, IE5
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		}
		xmlhttp.onreadystatechange=function()
		{
			if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{
				
			}
		}
		xmlhttp.open("GET","SetSessionAjax.aspx?q="+isVisible,true);
		xmlhttp.send();
	}
</script>
