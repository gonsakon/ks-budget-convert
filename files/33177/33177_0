	<script>
	$(function() {
		$( "#search_textfield" ).autocomplete({
			source:function(request, response){
				if(request.term.trim()!="" && request.term.trim()!="請輸入關鍵字"){
				var xmlhttp;
				if (window.XMLHttpRequest)
				{// code for IE7+, Firefox, Chrome, Opera, Safari
				xmlhttp=new XMLHttpRequest();
				}
				else
				{// code for IE6, IE5
				xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				}
				xmlhttp.onreadystatechange=function()
				{
					if (xmlhttp.readyState==4 && xmlhttp.status==200)
					{
						var availableTags=xmlhttp.responseText.split('|');
						response(availableTags);
					}
				}
				xmlhttp.open("GET","GetAutocompleteInfo.aspx?search_textfield="+request.term,true);
				xmlhttp.send();
				}
			}
		});
	});
	function setCookie(val)
	{
		var xmlhttp;
		if (window.XMLHttpRequest)
		{// code for IE7+, Firefox, Chrome, Opera, Safari
		xmlhttp=new XMLHttpRequest();
		}
		else
		{// code for IE6, IE5
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		}
		xmlhttp.onreadystatechange=function()
		{
			if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{
				location.reload();
			}
		}
		xmlhttp.open("GET","SetLayoutCookie.aspx?cookieVal="+val,true);
		xmlhttp.send();
	}
	</script>
