       </script>
	   <script type="text/javascript">
			
			$(document).ready(function(){
				var stringOfId = "ID";
				var intOfId = 0;
				$("td").each(function(){
					
					if(typeof($(this).attr("headers"))=="undefined"){
						
						$(this).attr("headers",stringOfId + intOfId);
						
					}
					intOfId = intOfId + 1;
					if(typeof($(this).attr("id"))=="undefined"){
						$(this).attr("id",stringOfId + intOfId);
					}
					
				});
			});
			$(document).ready(function(){
				$("a").each(function(){
					if($(this).text()==""){
						$(this).html($(this).html() + "&nbsp"); 
					}
					if($(this).text()==" "){
						$(this).html($(this).html() + "&nbsp"); 
					}
					if($(this).text().indexOf("<font>")){
						$(this).html($(this).html() + "&nbsp"); 
					}
					if(typeof($(this).attr("title"))=="undefined"){
						var str1 = $(this).children("img").attr("alt"); 
						var str2 = $(this).text();
						if(str1 != ""){
							$(this).attr("title",str1);
						}
						else if(str2 !=""){
							$(this).attr("title",str2);
						}
						else{
							$(this).attr("title",".");
						}
					}
					
				});
				
				
				$("img").each(function(){
					var strimgOn = "";
					strimgOn = $(this).attr("onclick");
					if(strimgOn != ""){
						if(typeof($(this).attr("onkeypress"))=="undefined"){
							$(this).attr("onkeypress",strimgOn);
						}
					}
					strimgOn = $(this).attr("src");
					if(strimgOn == "images/icon_4.gif"){
						$(this).attr("alt","");
					}
					if(strimgOn == "images/little01.gif"){
						$(this).attr("alt","");
					}
					if(strimgOn == "images/icon_1.gif"){
						$(this).attr("alt","");
					}
				});
				
				
			});
			
			/*$(document).ready(function() {
				$("*").each(function() {
