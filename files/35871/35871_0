        function SiteMenuMouseOver(strSubMenuID, strLinkID) {
            var objSubMenu = document.getElementById(strSubMenuID);
            var objLink = document.getElementById(strLinkID);
            var intTop = ElementClientTop(objLink);
            var intLeft = ElementClientLeft(objLink) + objLink.clientWidth - 2;
            MenuMouseOver(strSubMenuID, intTop, intLeft);
        }
        function SiteMenuMouseOver2(strSubMenuID, strLinkID) {
            var objSubMenu = document.getElementById(strSubMenuID);
            var objLink = document.getElementById(strLinkID);
            var intTop = ElementClientTop2(objLink);
            var intLeft = ElementClientLeft2(objLink);
            MenuMouseOver2(strSubMenuID, intTop, intLeft);
        }
        
