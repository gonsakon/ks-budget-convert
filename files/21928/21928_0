<script>
	
function doPrint(){
	newwin = window.open("","","scrollbars=yes,resizable=yes,toolbar=1"); 
	newwin.document.write("<html><head>"+$("head").html()+"</head><body style='height:10000px;'>"+$(".content").html()
		+"<br><center><a href='javascript:opener.printDetail();'><img src='/portal/tpl03/zh_TW/button/print.gif' alt='友善列印' border='0' /></a></center>"
		+"</body></html>");
}
function printDetail(){
	newwin.location.reload();
	newwin.print();
}

function mymedia(){ 	
  var myscreen=screen.availWidth;  
    if(myscreen<321){document.location.href="/index_pda.html"; return;}
}	
</script>

