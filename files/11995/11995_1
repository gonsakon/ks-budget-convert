<script type="text/javascript">
$(function(){
var $C3350_block = $('#C3350_bannerBlock'),
	$C3350_slides = $('ul.list', $C3350_block),
	$C3350_width = $C3350_slides.find('li').width(),
	$C3350_animateSpeed = 600,
	$C3350_animateTimeout = 7000,
	$C3350_stop = false,
	bannerTurn = '',
	$C3350_rightSize = ($C3350_slides.find('li').size()*$C3350_width-$C3350_block.width())*-1;
	// 移動主函式
	function C3350_move(){
		clearTimeout($C3350_timer);
		if(bannerTurn=='right'){
			$C3350_slides.css({left: $C3350_rightSize});
			$C3350_slides.stop().animate({
				left: '+='+$C3350_block.width()
			}, $C3350_animateSpeed, function(){
				$C3350_slides.stop().css({left: $C3350_rightSize});
				$C3350_slides.find('li:last').prependTo($C3350_slides);
				$C3350_slides.find('li:last').prependTo($C3350_slides);
				$C3350_slides.find('li:last').prependTo($C3350_slides);
				$C3350_slides.find('li:last').prependTo($C3350_slides);
				$C3350_slides.find('li:last').prependTo($C3350_slides);
				if(!$C3350_stop){
					$C3350_timer = setTimeout(C3350_move, $C3350_animateTimeout);
				}
			});
		}else{
			$C3350_slides.css({left: ''});
			$C3350_slides.stop().animate({
				left: '-='+$C3350_block.width()
			}, $C3350_animateSpeed, function(){
				$C3350_slides.stop().css({left: ""});
				$C3350_slides.find('li').eq(0).appendTo($C3350_slides);
				$C3350_slides.find('li').eq(0).appendTo($C3350_slides);
				$C3350_slides.find('li').eq(0).appendTo($C3350_slides);
				$C3350_slides.find('li').eq(0).appendTo($C3350_slides);
				$C3350_slides.find('li').eq(0).appendTo($C3350_slides);
				if(!$C3350_stop){
					$C3350_timer = setTimeout(C3350_move, $C3350_animateTimeout);
				}
			});
		}
	}
	// 移動列滑鼠事件
	$C3350_slides.hover(function(){
		$C3350_stop = true;
	}, function(){
		$C3350_stop = false;
		//C3350_move();
	});
	// 向左鍵事件
	$('#C3350_abgneBlock .prevten').click(function(){$C3350_slides.stop();bannerTurn='left';C3350_move();});
	// 向右鍵事件
	$('#C3350_abgneBlock .next').click(function(){$C3350_slides.stop();bannerTurn='right';C3350_move();});

	$C3350_timer = setTimeout(C3350_move, $C3350_animateTimeout);
	
});


</script>
 </div>
<div class="Centerbottom"><img src="xslGip/unit002/images/center_bottomleft.gif" alt="*"/></div>
</td>
</tr>
</table>
