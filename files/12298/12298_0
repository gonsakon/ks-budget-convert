<script>
    function ChangeMenuPic(value) {
        for (var i = 1; i <= 6; i++) {
            if (document.getElementById("m0" + i)) {
                document.getElementById("m0" + i).src = "../images/tw/m0" + i + ".jpg";
            }
        }
        document.getElementById("m0" + value).src = "../images/tw/m0" + value + "_1.jpg";
    }   
    </script>
