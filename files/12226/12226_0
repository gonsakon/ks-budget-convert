<script type="text/javascript">
    $(document).ready(function(){

		/*顯示自動完成提示字*/
		$("#keyword_q").autocomplete({
			source: function(request, response) {
				$.ajax({
					url: "http://www.tss.gov.tw/search_keyword_save.php",
					dataType: "json",
					data: {
						style: "tips",
						website: "1",
						term: request.term
					},
					success: function(data) {
						response(data);
					}
				});
			},
			minLength: 0
		});	
		
		
	  $(".block20141022-bu-close").click(function( event, a, b ) {
		  
		  //模擬click
		  if(event.status=="y"){
			  		  }
		  
		  if($("#fat-footer").is(':visible')){//是顯示時，要關閉
			  var set_fat_footer='n';//要保持關閉
			  
			  //開啟按鈕
			  $("#block20141022-btn").css({
				"background-image": "url(../img_fat_footer/block20141022-bu-open.gif)",
				"height": "30px"
			  });
			  
			  $(".block20141022-bu-close").attr({
				  title: "開啟"
				  });

			  $("#fat-footer").hide();
			  $("#block20141022-in").hide();
			 
		  }else{//是關閉時，要開啟
			  set_fat_footer='y';
			  
			  $("#fat-footer").show();
			  $("#block20141022-in").show();
			  
			  //圖檔更換為"關閉"的按鈕
			  $("#block20141022-btn").css({
				"background-image": "url(../img_fat_footer/block20141022_01.gif)",
				"height": "37px"
			  });

			  $("#block20141022-btn").attr('val','open');				
		   }

		  $.ajax({ 
			  type: "POST", 
			  data: "set_fat_footer=" + set_fat_footer,
			  dataType: "json", 
			  cache: false, 
			  url: 'search_keyword_save.php', 
			  error: function() { 
				  //alert("ajax 錯誤!!"); 
			  }, 
			  success: function(data) { 
				  //alert(data);
			  } 
		  });

		  return false;
	  });
	  
	  			
});

</script>


