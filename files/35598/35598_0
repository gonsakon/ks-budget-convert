    <div id="jsDetect">請開啟javascript</div>
    <ui-view></ui-view>
    <!--[if lt IE 10]>
          <script type="text/javascript">
            FileAPI = {
              jsUrl: 'lib/fileAPI/FileAPI.min.js',
