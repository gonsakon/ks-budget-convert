                $.each(HideControls, function (key, value) {
                    if ($(value).length > 0) {
                        $(value).hide();
                    }
                });

                var HideParentControls = new Array(".Pager");
                $.each(HideParentControls, function (key, value) {
                    if ($(value).length > 0) {
                        $(value).parent().hide();
                    }
                });
            }

        });
