   $(document).ready(function () { 
      $("#isearch").keyup(function (e) { 
           var code = (e.keyCode ? e.keyCode : e.which); 
           if (code == 13) { 
               location.href = "/zh/Copyright_10_7.htm?q=" + encodeURIComponent($(this).val()); 
           } 
       }); 
      $(".enter").click(function (e) { 
           location.href = "/zh/Copyright_10_7.htm?q=" + encodeURIComponent($("#isearch").val()); 
       }); 
