                    <script>
                        $(function () {
                            $(".mmenuin").hide();
							if (ismobile) {
								$(".mmenu").find("img").parent().attr("href", "javascript:void(0);").click(function () {
									$(this).parent().find(".mobilemenuflew").click();
								})		
							}
							
                            $(".mobilemenuflew").click(function () {
                                $(".mmenuin").find("li div").remove(); $(".mobilemenuflew").css("background", "url(../images/zh/m_flew.png) no-repeat");
                                if ($(this).parent().attr("id") != "active") {
                                    $(this).css("background", "url(../images/zh/m_flewUp.png) no-repeat");
                                    var _index = $(".mobilemenuflew").index(this);
                                    $(".mmenuin").hide(); $(".mmenu").height(50); $(".mmenu").attr("id", "active_off");
                                    $(".mmenuin").each(function (index) {
                                        if (index == _index) {
                                            $(this).show(); $(this).parent().attr("id", "active");
                                            $(this).parent().animate({ opacity: 1.0, height: ($(this).height() + 55) + "px" }, 1000);
                                        }
                                    })
                                } else {                                    
                                    $(this).parent().animate({ opacity: 1.0, height: "55px" }, 1000);
                                    $(".mmenu").attr("id", "active_off");
                                }
                            })
                        })
                    </script>					
