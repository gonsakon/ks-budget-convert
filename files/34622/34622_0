                    <input type="submit" name="ctl00$ctl02$epaperOrderCancel" value="取消訂閱" id="ctl00_ctl02_epaperOrderCancel" class="newsletterBtn_Cancel" />

                    
                    <script type="text/javascript">
                        $(document).ready(function () {
                            if (!ismobile) {
                                $("#newsletter").show();
                            }
							
                            var originalVal = null;
                            $('#newsletter input:not(input[type=submit])').each(function () {
                                $(this).focus(function () {
                                    originalVal = $(this).val();
                                    if ($(this).val() === originalVal) {
                                        $(this).val('');
                                    }
                                })
		                        .blur(function () {
		                            if ($(this).val() === '') {
		                                $(this).val(originalVal);
		                            }
		                        });
                            });

                            //$(".epaper_vailder").hide();

//                            $("#epaper_submit").click(function () {
//                                if (($("#ctl00_ctl02_epaper_email").val() == "") || ($("#ctl00_ctl02_epaper_email").val() == "  請輸入Email  ")) {
//                                    alert("請輸入Email");
//                                } else {
//                                    tb_show($("#epaper_submit").attr("title"), "#TB_inline?height=120&amp;width=250&amp;inlineId=epaper_vailer_submit");
//                                }
//                            })
                        });
                    </script>
