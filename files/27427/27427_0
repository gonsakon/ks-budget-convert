<script type="text/javascript">
$(function(){
var $ad_block = $('#ad_bannerBlock'),
	$ad_slides = $('ul.list', $ad_block),
	$ad_width = $ad_slides.find('li').width(),
	$ad_animateSpeed = 600,
	$ad_animateTimeout = 15000,
	$ad_stop = false,
	bannerTurn = '',
	$ad_rightSize = ($ad_slides.find('li').size()*$ad_width-$ad_block.width())*-1;
	// 移動主函式
	function ad_move(){
		clearTimeout($ad_timer);
		if(bannerTurn=='right'){
			$ad_slides.css({left: $ad_rightSize});
			$ad_slides.stop().animate({
				left: '+='+$ad_block.width()
			}, $ad_animateSpeed, function(){
				$ad_slides.stop().css({left: $ad_rightSize});
				$ad_slides.find('li:last').prependTo($ad_slides);
				$ad_slides.find('li:last').prependTo($ad_slides);
				$ad_slides.find('li:last').prependTo($ad_slides);
				$ad_slides.find('li:last').prependTo($ad_slides);
				$ad_slides.find('li:last').prependTo($ad_slides);
				if(!$ad_stop){
					$ad_timer = setTimeout(ad_move, $ad_animateTimeout);
				}
			});
		}else{
			$ad_slides.css({left: ''});
			$ad_slides.stop().animate({
				left: '-='+$ad_block.width()
			}, $ad_animateSpeed, function(){
				$ad_slides.stop().css({left: ""});
				$ad_slides.find('li').eq(0).appendTo($ad_slides);
				$ad_slides.find('li').eq(0).appendTo($ad_slides);
				$ad_slides.find('li').eq(0).appendTo($ad_slides);
				$ad_slides.find('li').eq(0).appendTo($ad_slides);
				$ad_slides.find('li').eq(0).appendTo($ad_slides);
				if(!$ad_stop){
					$ad_timer = setTimeout(ad_move, $ad_animateTimeout);
				}
			});
		}
	}
	// 移動列滑鼠事件
	$ad_slides.hover(function(){
		$ad_stop = true;
	}, function(){
		$ad_stop = false;
		//ad_move();
	});
	// 向左鍵事件
	$('#ad_abgneBlock .prevten').click(function(){$ad_slides.stop();bannerTurn='left';ad_move();});
	// 向右鍵事件
	$('#ad_abgneBlock .next').click(function(){$ad_slides.stop();bannerTurn='right';ad_move();});

	$ad_timer = setTimeout(ad_move, $ad_animateTimeout);
	
});


</script>
