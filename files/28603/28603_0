<script type="text/javascript">
    $(function () {
        RingOnLoad();
    });

    //設定 Cookie 相關值
    function SwitchFontSize(fs) {
        $.cookie('fontsize', fs);
        var s = document.getElementsByTagName("div");
        for (i = 0; s.length; i++) {
            if (s[i] == undefined)
                return;

            if (s[i].id != '') {
                s[i].style.fontSize = fs;
                $('.module_link_text_in li h5 a').css('font-size', '16px');
            }
        }
    }

    //設定載入時呼叫 Cookie 相關值
    function RingOnLoad() {
        var fs = $.cookie("fontsize");
        if (fs != null) {
            SwitchFontSize(fs);
        }
    }

</script>

<!--------------------字級 fontlevel---------------------->

